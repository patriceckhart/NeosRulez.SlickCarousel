prototype(Neos.Neos:Page) {
    @context.slickAllOver = ${Configuration.setting('NeosRulez.SlickCarousel.slickAllOver') ? true : false}
    head {
        NeosRulezSlickCarouselStyles = NeosRulez.ScssParser:ScssFile
        NeosRulezSlickCarouselStyles {
            source = 'resource://NeosRulez.SlickCarousel/Private/Styles/slick.scss'
            inline = FALSE
            format = 'compressed'
            outputFolder = 'resource://NeosRulez.SlickCarousel/Public/Styles/'
            @if.contentIsPresent = ${slickAllOver ? true : q(documentNode).children('[instanceof Neos.Neos:ContentCollection]').find('[instanceof NeosRulez.SlickCarousel:Component.SlickCarousel]').is()}
        }
        NeosRulezSlickCarouselJQuery = Neos.Fusion:Tag {
            tagName = 'script'
            attributes {
                src = Neos.Fusion:ResourceUri {
                    path = 'resource://NeosRulez.SlickCarousel/Public/JavaScript/jquery-3.2.1.min.js'
                }
            }
            @if.contentIsPresent = ${slickAllOver ? true : q(documentNode).children('[instanceof Neos.Neos:ContentCollection]').find('[instanceof NeosRulez.SlickCarousel:Component.SlickCarousel]').is()}
        }
        NeosRulezSlickCarouselScripts = NeosRulez.JsMinifier:JsFile
        NeosRulezSlickCarouselScripts {
            @position = 'end'
            source = 'resource://NeosRulez.SlickCarousel/Private/JavaScript/slick.js'
            inline = FALSE
            outputFolder = 'resource://NeosRulez.SlickCarousel/Public/JavaScript/'
            @if.contentIsPresent = ${slickAllOver ? true : q(documentNode).children('[instanceof Neos.Neos:ContentCollection]').find('[instanceof NeosRulez.SlickCarousel:Component.SlickCarousel]').is()}
        }
    }
}