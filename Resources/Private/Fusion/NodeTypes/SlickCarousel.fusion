prototype(NeosRulez.SlickCarousel:SlickCarousel) < prototype(Neos.Neos:Content) {
    templatePath = 'resource://NeosRulez.SlickCarousel/Private/Templates/NodeTypes/SlickCarousel.html'
    slickCarouselItems = Neos.Neos:ContentCollection {
        nodePath = 'slickCarouselItems'
        content.iterationName = 'slickCarouselItemsIteration'
    }
    showarrows = ${q(node).property('showarrows')}
    showdots = ${q(node).property('showdots')}
    carouselItemArray = ${q(node).children('slickCarouselItems').children('[instanceof Raw.Site:SlickCarouselItem]')}
}